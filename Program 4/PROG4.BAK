PROG4	CSECT
	USING	PROG4,15
*
***<STEP 1> PRINT THE HEADERS (LABELS)
*
LABELS	XPRNT	=C'          TANK DIMENSIONS'
	XPRNT	=C'+                       CAPACITY'
	XPRNT	=C'           (IN FEET)'
	XPRNT	=C'+                          (IN GALLONS)'
	XPRNT	=C'        LENGTH         WIDTH       DEPTH'
	XPRNT =C'+       VOLUME       80% FILLED'
*
***<STEP 2> INITIALIZE REGISTERS
*
RST	SR	2,2	EVEN FOR MULITPLY
	SR	9,9	HOLDS VOLUME
	SR	12,12	EVEN FOR DIVIDE
	SR	13,13	HOLDS PERCENT FILLED
	BC	B'1111',NUMS
*
***<STEP 3>
*
NUMS	XREAD	CARD,79	READ IN NUMBER
	BC	B'0100',DONE	BRANCH TO DONE IF EOF
	XDECI	3,CARD	CONVERT TO NUMBER
	XDECI	5,0(1)
	XDECI	7,0(1)
	XDECO	3,NUMLINE
	XDECO	5,NUMLINE+15
	XDECO	7,NUMLINE+27
*	XPRNT	NUMLINE,80
	BC	B'1111',VOLUME
*	BC	B'1111',NUMS
*
***
*
VOLUME	MR	2,5
	MR	2,7
	AR	9,3
	M	8,GAL
	D	8,HUN
	XDECO	9,NUMLINE+40
	BC	B'1111',FILLED
*	XPRNT	NUMLINE,80
*	BC	B'1111',RST
*	
FILLED	M	8,EGTY
	AR	13,9
	D	12,HUN
	XDECO	13,NUMLINE+53
	XPRNT	NUMLINE,80
	BC	B'1111',RST
DONE	BCR	B'1111',14	TERMINATE PROGRAM
*
********************************************************
*
CARD	DS	CL79
NUMLINE	DC	CL80'*'
GAL	DC	F'748'
HUN	DC	F'100'
EGTY	DC	F'80'
	END PROG4
$ENTRY	DATA4.TXT
