PROG5	CSECT
	USING PROG5,15
	SR	4,4
	LA	3,TABLE
*
	XPRNT	=C'      TABLE VALUES'
*
READ	XREAD	CARD,80
	BC	B'0100',DONE	BRANCH TO DONE IF EOF
	XDECI	2,CARD
	XDECO	2,NUMLINE
	XPRNT	NUMLINE,80
	ST	2,0(3)
	LA	4,1(4)
	LA	3,4(3)
	BC	B'1111',READ
*
DONE	BCR	B'1111',14	TERMINATE PROGRAM
*
*****************************************************
*
TABLE	DS	40F
CARD	DS	CL80	CARD INPUT AREA
NUMLINE	DC	CL80'*'
*
	END	PROG5
$ENTRY	DATA5.TXT
